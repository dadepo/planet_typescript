<html>
    <head>
        <script type="text/javascript">
        </script>
    </head>
    <body>
        <% if (links.length > 0) { %>
            <table>
                <tr>
                  <th>id</th>
                  <th>title</th>
                  <th>Status</th>
                  <th>Actions</th>
                </tr>
            
                <% links.forEach(function(link){ %>
                    <tr>
                        <td><%= link.id %></td>
                        <td>link.source</td>
                        <td>visible</td>
                        <td>Hide</td>
                    </tr>
                <% }); %>

              </table>
          <% } else {%>
            No posts yet
        <% } %>
    </body>
</html>