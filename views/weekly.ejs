<html>
    <head>
        <link rel="stylesheet" href="./style/home.css">
        <%- await include('views/partials/ga.ejs') %>
        <%- await include('views/partials/voter.ejs') %>
        <title>Home</title>
    </head>
    <body>
    <%- await include('views/partials/header.ejs', {origin}) %>
        <main>
        <article>
            <nav>
                <%- await include('views/partials/nav.ejs', {origin}) %>
            </nav>

            <% if (links.length > 0) { %>

                <% for(var i=0; i < links.length; i++) { %>
                    <div class="link_wrapper">
                        <div class="the_link">
                            <span><%= links[i].id%></span>
                            <a class="go_link" href="./weekly/<%= links[i].id %>" title="<%= links[i].title %>"><%- links[i].title %></a>
                            <span><a class="no_underline_link" href="./weekly/<%= links[i].id %>">View posts</a></span>
                        </div>
                    </div>

                <% } %>
<!--                <div class="more" ><a class="no_underline_link" href="?page=<%= page %>">More</a></div>-->
            <% } else {%>
                No posts yet
            <% } %>


        </article>
        <aside>
            <%- await include('views/partials/side.ejs', {currentUser}) %>
        </aside>
        </main>
        <%- await include('views/partials/footer.ejs') %>
    </body>
</html>
